{
  "name": "w1ch0_blog",
  "description": "w1ch0_blog",
  "version": "0.0.1",
  "scripts": {
    "dev": "run-p start:dev watch:css",
    "build": "npm run build:css && sapper build --legacy",
    "watch:css": "postcss static/global.css -o static/index.css -w",
    "build:css": "NODE_ENV=production postcss static/global.css -o static/index.css",
    "export": "npm run build && sapper export --legacy",
    "start": "node __sapper__/build",
    "start:dev": "sapper dev",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "test": "run-p --race dev cy:run"
  },
  "dependencies": {
    "@types/compression": "^1.7.0",
    "@types/express": "^4.17.8",
    "@types/marked": "^1.1.0",
    "@types/polka": "^0.5.1",
    "compression": "^1.7.1",
    "express": "^4.17.1",
    "gray-matter": "^4.0.2",
    "marked": "^1.1.1",
    "polka": "next",
    "sirv": "^1.0.0",
    "svelte-icons": "^2.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/runtime": "^7.0.0",
    "@rollup/plugin-babel": "^5.0.0",
    "@rollup/plugin-commonjs": "^14.0.0",
    "@rollup/plugin-node-resolve": "^8.0.0",
    "@rollup/plugin-replace": "^2.2.0",
    "@types/node": "^14.6.3",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^7.1.1",
    "postcss-import": "^12.0.1",
    "postcss-load-config": "^2.1.0",
    "postcss-nested": "^4.2.3",
    "rollup": "^2.3.4",
    "rollup-plugin-svelte": "^5.2.3",
    "rollup-plugin-terser": "^6.1.0",
    "rollup-plugin-typescript2": "^0.27.2",
    "sapper": "^0.28.0",
    "svelte": "^3.17.3",
    "svelte-preprocess": "^4.1.1",
    "tailwindcss": "^1.7.3",
    "typescript": "^4.0.2"
  }
